#include <stdio.h>
#include <stdlib.h>
#include <math.h>

struct triangle
{
	int a;
	int b;
	int c;
};

typedef struct triangle triangle;
long long area2(triangle t) {
    long long a = t.a, b = t.b, c = t.c;

    long long s2 = a + b + c;  // actually 2*s

    // area^2 * 16 = (2s)(2s-2a)(2s-2b)(2s-2c)
    long long A = s2 * (s2 - 2*a) * (s2 - 2*b) * (s2 - 2*c);

    return A;  // safe to compare
}

// --- Sort function ---
void sort_by_area(triangle *tr, int n) {
    for (int i = 0; i < n - 1; i++) {
        for (int j = i + 1; j < n; j++) {
            if (area2(tr[i]) > area2(tr[j])) {
                triangle temp = tr[i];
                tr[i] = tr[j];
                tr[j] = temp;
            }
        }
    }
}

int main()
{
	int n;
	scanf("%d", &n);
	triangle *tr = malloc(n * sizeof(triangle));
	for (int i = 0; i < n; i++) {
		scanf("%d%d%d", &tr[i].a, &tr[i].b, &tr[i].c);
	}
	sort_by_area(tr, n);
	for (int i = 0; i < n; i++) {
		printf("%d %d %d\n", tr[i].a, tr[i].b, tr[i].c);
	}
	return 0;
}
